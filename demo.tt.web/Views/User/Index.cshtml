@{
    /**/

    ViewBag.Title = "User Page";
}
<div class="row">
    <div class="col-md-4">
        <h2>Users</h2>
        <table>
            <tr>
                <th></th>
                <th> Id </th>
                <th> Name </th>
            </tr>

            @{ foreach (var item in Model)
                {
                    <tr>
                        <td> @item.Id </td>
                        <td> @item.Name </td>
                    </tr>
                }
            }
        </table>
    </div>

</div>